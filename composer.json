{
    "name": "dotkernel/dot-hydrator",
    "description": "DotKernel hydrator component extending and customizing laminas-hydrator",
    "authors": [
        {
            "name": "DotKernel Team",
            "email": "team@dotkernel.com"
        }
    ],
    "type": "library",
    "license": "MIT",
    "keywords": [
        "hydrator",
        "services",
        "factories",
        "container",
        "zf2",
        "zf3",
        "service-manager"
    ],
    "require": {
        "php": "^7.2",
        "laminas/laminas-servicemanager": "^3.3.0",
        "laminas/laminas-hydrator": "^2.4",
        "laminas/laminas-dependency-plugin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7",
        "squizlabs/php_codesniffer": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "Dot\\Hydrator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DotTest\\Hydrator\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev",
            "dev-develop": "1.2-dev"
        }
    }
}
