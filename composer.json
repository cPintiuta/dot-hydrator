{
  "name": "dotkernel/dot-hydrator",
  "description": "Adapts zend-hydrator to DotKernel applications",
  "authors": [
    {
      "name": "n3vrax",
      "email": "tibi@apidemia.com"
    }
  ],
  "type": "library",
  "license": "MIT",
  "keywords": [
    "hydrator",
    "services",
    "factories",
    "container",
    "zf2",
    "zf3",
    "service-manager"
  ],
  "require": {
    "php": "^7.1",
    "zendframework/zend-servicemanager": "^3.0",
    "zendframework/zend-hydrator": "^2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.7",
    "squizlabs/php_codesniffer": "^2.5"
  },
  "autoload": {
    "psr-4": {
      "Dot\\Hydrator\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DotTest\\Hydrator\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.6-dev"
    }
  }
}
